{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "willsides/cover-link",
	"version": "1.1.0",
	"title": "Cover Link",
	"category": "design",
	"icon": "cover-image",
	"description": "A post link with text overlaid on the thumbnail",
	"supports": {
		"html": false,
        "color": {
            "background": true,
			"text": false
        }
	},
	"attributes": {
		"page": {
			"type": "object",
            "default": "none"
		},
		"aspectRatio": {
			"type" : "string",
			"default": "16/9"
		}
	},
	"textdomain": "cover-link",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}